#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/sh" ]; then
  "$basedir/sh"  "$basedir/../tinypng/bin/tinypng" "$@"
  ret=$?
else 
  sh  "$basedir/../tinypng/bin/tinypng" "$@"
  ret=$?
fi
exit $ret
